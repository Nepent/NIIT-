<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
   <servlet>
  	<servlet-name>AdminServlet</servlet-name>
  	<display-name>AdminServlet</display-name>
  	<description></description>
  	<servlet-class>com.niit.servlet.AdminServlet</servlet-class>
  </servlet>
   <servlet>
   	<servlet-name>BookService</servlet-name>
   	<display-name>BookService</display-name>
   	<description></description>
   	<servlet-class>com.niit.service.BookService</servlet-class>
   </servlet>
   <servlet>
   	<servlet-name>BookServlet</servlet-name>
   	<display-name>BookServlet</display-name>
   	<description></description>
   	<servlet-class>com.niit.servlet.BookServlet</servlet-class>
   </servlet>
  <servlet-mapping>
  	<servlet-name>AdminServlet</servlet-name>
  	<url-pattern>/AdminServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>BookService</servlet-name>
  	<url-pattern>/BookService</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>BookServlet</servlet-name>
  	<url-pattern>/BookServlet</url-pattern>
  </servlet-mapping>
</web-app>
